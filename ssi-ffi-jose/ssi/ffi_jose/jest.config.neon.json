{
  "testMatch": [
    "**/test/**/?(*.)+(spec|test).[jt]s?(x)"
  ],
  "coverageDirectory": "reports/junit/jest/coverage",
  "collectCoverageFrom": [
    "**/lib/**/*.js"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 100,
      "functions": 100,
      "lines": 100,
      "statements": 100
    }
  }
}
